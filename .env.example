# SheetDB API Configuration
# Copy this file to .env and replace with your own SheetDB API endpoints

# Airlines Database API (for fetching airline information)
SHEETDB_AIRLINES_API=https://sheetdb.io/api/v1/YOUR_AIRLINES_API_KEY

# Airline Names API (for searching by airline name)
SHEETDB_AIRLINE_NAMES_API=https://sheetdb.io/api/v1/YOUR_AIRLINE_NAMES_API_KEY

# User Registration/Login API
SHEETDB_USERS_API=https://sheetdb.io/api/v1/YOUR_USERS_API_KEY

# Active Sessions API (for tracking logged-in users)
SHEETDB_SESSIONS_API=https://sheetdb.io/api/v1/YOUR_SESSIONS_API_KEY

# Bookings API (for flight bookings)
SHEETDB_BOOKINGS_API=https://sheetdb.io/api/v1/YOUR_BOOKINGS_API_KEY

# Admin Panel API (legacy/optional)
SHEETDB_ADMIN_API=https://sheetdb.io/api/v1/YOUR_ADMIN_API_KEY
